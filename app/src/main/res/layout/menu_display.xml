                        android:layout_height="match_parent"
                        android:layout_gravity="center_horizontal"
                        android:animateLayoutChanges="true"
                        android:orientation="vertical">

                        <LinearLayout
                            android:id="@+id/cncTokenLL"
                            android:layout_width="match_parent"
