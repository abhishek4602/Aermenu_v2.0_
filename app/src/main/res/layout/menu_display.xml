                                app:cardCornerRadius="12dp"
                                app:cardElevation="5dp"
                                app:layout_constraintBottom_toBottomOf="@+id/textView311"
                                app:layout_constraintEnd_toStartOf="@+id/textView311"
                                app:layout_constraintStart_toStartOf="parent"
