                                android:fontFamily="@font/msbold"
                                android:text="Veg Only"
                                android:textAllCaps="true"
                                android:textSize="12sp" />

                        </LinearLayout>

                        <ScrollView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent">

                            <LinearLayout
